{
    "version": 2,
    "builds": [
      {
        "src": "weather_project/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },
      { 
        "src": "/public/static",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "weather_project/wsgi.py"
      }
    ],
    "static": {
      "source": "staticfiles", 
      "destination": "/public/static"
    }
  }
  